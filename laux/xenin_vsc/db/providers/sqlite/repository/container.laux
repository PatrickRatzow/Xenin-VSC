public class XeninVSC.SQLite.ContainerRepository extends XeninVSC.Repository
    public setup()
        self:addDAO(XeninVSC.SQLite.ContainerDAO)
    end

    public getContainer(id)
        return self.containerDAO:getContainer(id)
    end

    public insertContainer(id)
        return self.containerDAO:insertContainer(id)
    end
end

